@page
@model GroupCCP.Pages.ComplaintReceiveMeanss.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ComplaintReceiveMeans[0].Means)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ComplaintReceiveMeans) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Means)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.MeansId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.MeansId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.MeansId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
